{
  "languageserver": {
    "dockerfile": {
      "command": "/usr/bin/docker-langserver",
      "filetypes": ["dockerfile"],
      "args": ["--stdio"]
    }
  },
  "rust-analyzer.server.path": "/usr/lib/rustup/bin/rust-analyzer",
  "python.formatting.provider": "black",
  "python.formatting.blackPath": "/usr/bin/black",
  "clangd.fallbackFlags": ["-std=c++20"],
  "suggest.invalidInsertCharacters": [" ", "(", "<", "{", "[", "\r", "\n", "\t"],
  "suggest.noselect": true
}
